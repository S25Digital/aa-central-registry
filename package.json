{
  "name": "@s25digital/aa-central-registry",
  "version": "2.4.0",
  "description": "A package to work with Sahamati Central Registry in AA ecosystem",
  "main": "dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "prebuild": "rimraf dist",
    "build": "node build.js",
    "build:types": "tsc",
    "prepublishOnly": "npm run build",
    "test": "TZ=utc DOTENV_CONFIG_PATH=.env.spec mocha -r esbuild-register -r dotenv/config 'test/**/*.spec.ts' --timeout 10000",
    "test:coverage": "nyc npm run test",
    "preinteg": "npm run build",
    "integ": "node -r dotenv/config integ/index.js"
  },
  "keywords": [
    "account-aggregator",
    "central registry",
    "aa-commons",
    "AA"
  ],
  "author": "Simranjeet Singh",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pino": "^9.7.0",
    "redis": "^5.1.1",
    "rsa-pem-from-mod-exp": "^0.8.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.24",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "chai": "^5.2.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "mocha": "^11.5.0",
    "nock": "^14.0.4",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-json-decoder": "^1.0.11"
  }
}
